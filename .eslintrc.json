{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
      "standard-with-typescript", 
      "prettier", 
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
      
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint", "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
}
